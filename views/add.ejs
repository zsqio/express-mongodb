<!-- 添加数据页 -->
<%- include('header') %>
<div class="bs-docs-header" id="content">
  <div class="container">
    <h1>添加一条数据</h1>
  </div>
  <div class="container">
    <div class="row">
      <form action="/add" method="post" class="form-horizontal">
        <div class="form-group">
          <label for="inputUid" class="col-sm-1 control-label">uid:</label>
          <div class="col-sm-5">
            <input type="text" name="uid" id="inputUid" placeholder="uid" class="form-control">
          </div>
        </div>
        <div class="form-group">
          <label for="inputTitle" class="col-sm-1 control-label">title:</label>
          <div class="col-sm-5">
            <input type="text" name="title" id="inputTitel" placeholder="title" class="form-control">
          </div>
        </div>
        <div class="form-group">
          <label for="inputContent" class="col-sm-1 control-label">content:</label>
          <div class="col-sm-5">
            <input type="text" name="content" id="inputContent" placeholder="content" class="form-control">
          </div>
        </div>
        <div class="form-group">
          <div class="col-sm-5 col-sm-offset-1">
            <input type="submit" class="btn btn-primary" value="提交">
            <input type="reset" class="btn btn-cancel"value="重置">
          </div>
        </div>
      </form>
    </div>
  </div>
</div>
<%- include('footer') %>
