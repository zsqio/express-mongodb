<!-- 修改页-->
<%- include('header') %>
<div class="bs-docs-header" id="content">
  <div class="container">
    <h1>修改数据</h1>
  </div>
</div>
<div class="container">
  <div class="row">
    <form action="" method="post"role="form" class=form-horizontal>
      <div class="form-group">
        <label for="" class="col-sm-1 control-label">uid:</label>
        <div class="col-sm-5">
          <input type="text" name="uid" value="<%= demo.uid%>" class="form-control">
        </div>
      </div>
      <div class="form-group">
        <label for="" class="col-sm-1 control-label">title:</label>
        <div class="col-sm-5">
          <input type="text" name="title" value="<%= demo.title%>" class="form-control">
        </div>
      </div>
      <div class="form-group">
        <label for="" class="col-sm-1 control-label">content:</label>
        <div class="col-sm-5">
          <input type="text" name="content" value="<%= demo.content%>" class="form-control">
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-5 col-sm-offset-1">
          <input type="hidden" name="id" value="<%= demo.id%>">
          <input type="submit" value="提交" class="btn btn-primary">
          <input type="reset" value="重置" class="btn btn-cancel">
        </div>
      </div>
    </form>
  </div>
</div>
<%- include('footer') %>
